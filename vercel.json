{
    "builds": [{
        "src": "judges_review/judges_review/wsgi.py",

        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    { "src": "backend/**", "use": "@vercel/python" } ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "judges_review/judges_review/wsgi.py"
        }
    ]
}